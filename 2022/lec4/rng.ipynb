    "### code we wrote in class\n",
    "def U(x):\n",
    "    return (16807*x + 0)%(2**31 - 1)\n",
    "\n",
    "x = 11.0  \n",
    "xs = []\n",
    "for i in range(20000):\n",
    "    x = U(x)\n",
    "    xs = xs + [x/(2**31 -1)]\n",
    "    #print(x)\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "plt.hist(xs, bins = 100, ec= 'black') "
